- provide(:title, 'Create a new initiative')
%h1
  Create a new doo-dah
.row
  .span6.offset3
    = form_for(@initiative) do |f|
      = render 'shared/error_messages', object: f.object
      = f.label :name
      = f.text_field :name

      = f.label :description
      = f.text_field :description

      = f.submit "Create", class: "btn btn-large btn-primary"
